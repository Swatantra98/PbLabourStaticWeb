@if (Model.Documents.Any())
{
    <section class="vs-about-us-wrap vs-about-us-layout3 pt-60 pt-lg-130 pb-30 pb-lg-100" id="about">
        <div class="container">
            <div class="row gutters-50">
                <div class="col-lg-6">
                    <div class="widget widget_categories">
                        <h3 class="widget_title WhtNew">What's New</h3>
                        <ul>
                            @foreach (var item in Model.Documents)
                            {
                                <li data-toggle="tooltip" title="@item.DocumentName">
                                    <p><a href="@ViewBag.pbLabourAdminStaticBaseUrl/uploads/@item.UploadedFileName" target="_blank">
                                        @item.DocumentName
                                    </a></p> <span> @(countWhatsNew++)</span>
                                </li>
                            }
                            <a href="@ViewBag.StaticSitePath/Profile/Notification" class="link-btn More" tabindex="0">
                                For More<i class="fas fa-long-arrow-right"></i>
                            </a>
                        </ul>
                    </div>
                </div>

                <div class="col-lg-6 align-self-center mb-30">
                    <div class="widget widget_categories">
                        <h3 class="widget_title NewNoti">New Notifications</h3>
                        <ul>
                            @foreach (var item in Model.Documents)
                            {
                                <li data-toggle="tooltip" title="@item.DocumentName">
                                    <p><a href="@ViewBag.pbLabourAdminStaticBaseUrl/uploads/@item.UploadedFileName" target="_blank">
                                        @item.DocumentName
                                    </a></p> <span> @(countNewNoti++)</span>
                                </li>
                            }
                            <a href="@ViewBag.StaticSitePath/Profile/Notification" class="link-btn More1" tabindex="0">
                                For More<i class="fas fa-long-arrow-right"></i>
                            </a>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
}
