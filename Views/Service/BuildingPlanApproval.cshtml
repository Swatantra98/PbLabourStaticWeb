<style>      .GIGWMenu {
    width: 100%;
    display: none;
    height: 150px;
    margin-top: 115px;
    position: fixed;
    z-index: 9999999;
}
    .inner-list ul li:before {
        content: none;
        font-family: "Font Awesome 5 Pro";
        position: absolute;
        left: 0;
        display: inline-block;
        width: 35px;
        height: 35px;
        box-shadow: 0px 5px 20px 0px rgb(17 180 245 / 15%);
        text-align: center;
        line-height: 35px;
        font-size: 14px;
        color: rgb(68, 192, 196);
        font-weight: 400;
        background-color: #fff;
        border-radius: 50%;
    }
    .marginClass {
        width: 100%;
        margin-left: auto;
        margin-right: auto;
    }

    .requireCon {
        margin-top: -50px;
    }

    .imgCon {
        width: 5%;
        float: left;
    }

    .imgCon1 {
        width: 7%;
        float: left;
    }

    p {
        margin-bottom: 0px;
    }

    .textCon {
        margin-left: 50px;
        margin-top: 15px;
    }

    .flowChartCon {
        margin-top: -50px;
        margin-bottom: -130px;
    }

    .process-layout4 .vs-process {
        position: relative;
        padding: 15px 40px 28px 15px;
        margin-bottom: 15px;
        height: 300px;
        border: 1px solid rgb(68, 192, 196);
    }

    .bg-light {
        background-color: rgb(165 229 255 / 7%) !important;
    }

    .mainCon {
        border: 1px solid rgba(17, 180, 245, 0.3);
        background-color: rgba(17, 180, 245, 0.07);
        width: 63%;
        margin-left: auto;
        margin-right: auto;
        margin-bottom: 20px;
        padding: 20px
    }

    @@media only screen and (max-width: 600px) {
        .title1 {
            font-size: 32px;
        }

        .requireCon {
            margin-top: 0px;
        }

        .imgCon {
            width: 20%;
        }
           .imgCon1 {
        width: 20%;
    }
        .textCon {
            text-align: left;
            margin-left: 0px;
            margin-top: 30px;
        }

        .flowChartCon {
            margin-top: 0px;
            margin-bottom: -130px;
        }

        .process-layout4 .vs-process {
            position: relative;
            padding: 15px 40px 28px 15px;
            margin-bottom: 15px;
            height: 300px;
            border: 1px solid rgb(68, 192, 196);
        }

        .mainCon {
            width: 90%
        }
    }

</style>
